{
  "editor.rulers": [80, 120],
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnType": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.console.fontSize": 12,
  "editor.fontFamily": "'SauceCodePro NFM', 'SauceCodePro Nerd Font Mono', 'monospace', monospace",
  "editor.fontSize": 12,
  "editor.inlineSuggest.enabled": true,
  "editor.renderWhitespace": "boundary",
  "editor.tabSize": 2,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "json.format.enable": false,
  "jupyter.askForKernelRestart": false,
  "jupyter.notebookFileRoot": "${workspaceFolder}",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "python.formatting.provider": "black",
  "sqltools.disableChordKeybindings": true,
  "sqltools.disableReleaseNotifications": true,
  "terminal.integrated.automationProfile.linux": {
    "path": "/bin/bash"
  },
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.gpuAcceleration": "canvas",
  "window.titleBarStyle": "custom",
  "window.title": "${folderName}${separator}${activeEditorMedium}",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Low Eye Strain Dark",
  "workbench.colorCustomizations": {
    "editorWhitespace.foreground": "#2b2b2b"
  },
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.showTabs": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "git-rebase-todo": "default"
  },
  "workbench.layoutControl.type": "menu",
  "workbench.list.multiSelectModifier": "alt",
  "workbench.startupEditor": "newUntitledFile",
  "vim.useSystemClipboard": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true
  },
  "gitlens.rebaseEditor.ordering": "asc",
  "svg.preview.mode": "svg",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false
}
