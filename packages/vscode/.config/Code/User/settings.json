{
  "editor.rulers": [80, 120],
  "editor.lineNumbers": "relative",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnType": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.console.fontSize": 12,
  "editor.fontFamily": "'SauceCodePro NFM', 'SauceCodePro Nerd Font Mono', 'monospace', monospace",
  "editor.fontSize": 12,
  "editor.inlineSuggest.enabled": true,
  "editor.renderWhitespace": "boundary",
  "editor.tabSize": 2,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git-graph.repository.useMailmap": true,
  "json.format.enable": false,
  "jupyter.askForKernelRestart": false,
  "jupyter.notebookFileRoot": "${workspaceFolder}",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "python.formatting.provider": "black",
  "sqltools.disableChordKeybindings": true,
  "sqltools.disableReleaseNotifications": true,
  "terminal.integrated.automationProfile.linux": {
    "path": "/bin/bash"
  },
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.gpuAcceleration": "canvas",
  "window.titleBarStyle": "custom",
  "window.title": "${folderName}${separator}${activeEditorMedium}",
  "workbench.colorTheme": "Default Dark+",
  "workbench.colorCustomizations": {
    "editorWhitespace.foreground": "#2b2b2b"
  },
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.showTabs": "single",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "git-rebase-todo": "default"
  },
  "workbench.layoutControl.type": "menu",
  "workbench.list.multiSelectModifier": "alt",
  "workbench.startupEditor": "newUntitledFile",
  "vim.useSystemClipboard": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "yaml": true
  },
  "gitlens.rebaseEditor.ordering": "asc",
  "svg.preview.mode": "svg",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "remote.autoForwardPortsSource": "hybrid",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "window.newWindowProfile": "Default",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "sqltools.useNodeRuntime": true,
  "chat.editing.alwaysSaveWithGeneratedChanges": true,
  "chat.tools.terminal.autoApprove": {
    "make typecheck": {
      "approve": true,
      "matchCommandLine": true
    },
    "make test": {
      "approve": true,
      "matchCommandLine": true
    }
  }
}